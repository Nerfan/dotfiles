{
   "background": {
      "js": "background.js",
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "logo.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "search/search.css" ],
      "include_globs": [ "http://www.google.*/*", "https://www.google.*/*" ],
      "js": [ "search/jquery.js", "lib/cached.js", "promise.min.js", "request.js", "search/search.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Displays a warning if the site you are browsing is affected by the Heartbleed bug",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "256": "icon256.png",
      "48": "icon48.png",
      "512": "icon512.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Chromebleed",
   "options_page": "options.html",
   "permissions": [ "notifications", "activeTab", "tabs", "\u003Call_urls>" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1",
   "web_accessible_resources": [ "logo.png", "icon48.png", "logo-err48.png", "logo-ok48.png" ]
}
